    <%--print report--%>
    <script>
        $(document).ready(function () {
            $('.printReport').on("click", function () {
                var hrefLink = $(this).data("href");
                $('#modalPrinting').find(".modal-content").load(hrefLink);
            });

            OpenAccountInfo();
        });


        function OpenAccountInfo() {
            if ($("input[type='hidden'][id$='IsOpen']").length > 0)
                $("#account-info").click();
        }
    </script>

