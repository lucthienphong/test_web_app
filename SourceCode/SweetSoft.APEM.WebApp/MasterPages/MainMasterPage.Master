    <style type="text/css">
        #notification .scrollmsg:empty
        {
            display: none;
        }

        .scrollmsg
        {
            height: auto;
            max-height: 180px;
            width: 250px;
            overflow-x: hidden;
        }

        .listmsg
        {
            padding: 0;
            margin: 0;
        }

        .window_panel .window_footer
        {
            height: auto;
        }

        .allmsg
        {
            margin-left: 15px;
        }

        .listmsg > li > a
        {
            display: block;
            padding: 3px 20px;
            clear: both;
            font-weight: normal;
            line-height: 1.42857143;
            color: #333;
        }

            .listmsg > li > a:hover, .listmsg > li > a:focus
            {
                text-decoration: none;
                color: #262626;
                background-color: #f5f5f5;
            }

        .dropdown-menu > .window
        {
            margin-left: 20px;
            margin-top: 5px;
            display: block;
        }

        .flaticon-new100:before
        {
            font-size: 28px !important;
        }

        .badge
        {
            position: relative;
            top: -7px;
            right: 0px;
        }
    </style>
